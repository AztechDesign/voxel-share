{
  "name": "voxel-share",
  "description": "Take a snapshot of voxel.js and share on imgur/twitter",
  "version": "0.3.0",
  "homepage": "https://github.com/shama/voxel-share",
  "author": {
    "name": "Kyle Robinson Young",
    "email": "kyle@dontkry.com",
    "url": "http://dontkry.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/shama/voxel-share.git"
  },
  "bugs": {
    "url": "https://github.com/shama/voxel-share/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shama/voxel-share/blob/master/LICENSE-MIT"
    }
  ],
  "engines": {
    "node": ">= 0.8.0"
  },
  "scripts": {
    "start": "cd example && ../node_modules/.bin/browservefy world.js 8080 -- -d",
    "gh-pages": "cp -R ./example/* ./ && browserify world.js -o world.js"
  },
  "dependencies": {},
  "devDependencies": {
    "voxel-engine": "~0.3.2",
    "voxel-perlin-terrain": "0.0.5",
    "voxel-forest": "0.0.0",
    "browservefy": "0.0.9",
    "voxel-sky": "~0.1.0",
    "toolbar": "0.0.2",
    "voxel-debris": "0.0.4"
  },
  "keywords": [
    "voxel",
    "share",
    "imgur",
    "twitter"
  ]
}
